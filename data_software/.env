# Platform payout rates
PLATFORM_PAYOUT_SPOTIFY=0.0024
PLATFORM_PAYOUT_BEATPORT=1.25

# Database settings
DB_PATH=data/revenue.db
DATABASE_URL=sqlite:///data/revenue.db

# Prophet settings
PROPHET_CHANGEPOINT_PRIOR_SCALE=0.05
PROPHET_SEASONALITY_PRIOR_SCALE=10.0
PROPHET_SEASONALITY_MODE=multiplicative
PROPHET_HOLIDAYS_PRIOR_SCALE=10.0

# XGBoost settings
XGB_MAX_DEPTH=6
XGB_LEARNING_RATE=0.1
XGB_N_ESTIMATORS=100

# LSTM settings
LSTM_UNITS=50
LSTM_DROPOUT=0.2
LSTM_EPOCHS=100
LSTM_BATCH_SIZE=32

# Additional settings
LOG_LEVEL=INFO
LOG_FILE=app.log
FORECAST_HORIZON=12
TRAIN_TEST_SPLIT=0.8
RANDOM_STATE=42
